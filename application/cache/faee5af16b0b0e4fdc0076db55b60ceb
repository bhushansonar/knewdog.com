        SELECT *, GROUP_CONCAT(DISTINCT newsletter_category.en ORDER BY newsletter_category.en) as g_en, GROUP_CONCAT(DISTINCT newsletter_keyword.en ORDER BY newsletter_keyword.newsletter_keyword_id) as g_keyword_name, `language`.`language_longform` as newsletter_language
FROM (`newsletter`)
LEFT JOIN `newsletter_category` ON `FIND_IN_SET`(`newsletter_category`.`newsletter_category_id` , `newsletter`.`newsletter_category_id`) > 0
LEFT JOIN `newsletter_keyword` ON `FIND_IN_SET`(`newsletter_keyword`.`newsletter_keyword_id` , `newsletter`.`newsletter_keyword_id`) > 0
LEFT JOIN `newsletter_rate` ON `newsletter_rate`.`join_newsletter_id` = `newsletter`.`newsletter_id`
LEFT JOIN `language` ON `language`.`language_id` = `newsletter`.`language_id`
WHERE `newsletter`.`status` =  'Active'
AND `newsletter_relation` =  'parent'
GROUP BY `newsletter_id`
ORDER BY `newsletter_id` DESC
LIMIT 3<aside id="sidebar">
